/* mec_assy6906 EVB with MEC1723N-SZ daughter card
 * MEC1723 has internal 2KB EEPROM and controller.
 * Internal pins are disabled to reduce leakage.
 */

/ {
	aliases {
		eeprom-0 = &eeprom;
	};
};

&eeprom {
	status = "okay";
	pinctrl-0 = < &pspi_cs_n_gpio116
		      &pspi_clk_gpio117
		      &pspi_sdi_gpio075
		      &pspi_sdo_gpio074 >;
	pinctrl-names = "default";
};
