/* MEC5 EEPROM controller with internal 8 kilobyte EEPROM */
eeprom: eeprom@40002c00 {
	compatible = "microchip,mec5-eeprom";
	reg = <0x40002c00 0x400>;
	interrupts = <155 2>;
	interrupt-names = "done";
	girqs = <18 13>;
	size = <8192>;
	page-size = <32>;
	status = "disabled";
};
